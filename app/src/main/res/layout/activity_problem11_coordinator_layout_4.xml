<?xml version="1.0" encoding="utf-8"?>
<com.example.leeeyou.fixmyproblem.custom_view.StickyNavLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_problem11__coordinator_layout_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@id/id_stickynavlayout_topview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#4400ff00">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="256dp"
            android:gravity="center"
            android:text="Hehedada"
            android:textSize="30sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <TextView
        android:id="@id/id_stickynavlayout_indicator"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#98ffff" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/id_stickynavlayout_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


</com.example.leeeyou.fixmyproblem.custom_view.StickyNavLayout>
